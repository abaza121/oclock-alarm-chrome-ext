{
    "manifest_version": 3,
    "name": "BBC Theme Extension",
    "description": "Plays BBC Theme every hour",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "stopwatch.png"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
      },
    "permissions": ["offscreen","alarms"]
  }